#include <iostream>
#include <cmath>
using namespace std;

double F(double x) {
    return sqrt(2*x*x + 1.2 - cos(x)) - 1;
}

int main() {
    double A = 0;
    double B = 1; 
    
    double eps = 0.0001;
    double D;
    int counter = 0;
    
    if(F(A) * F(B) >= 0){
        cout<<"no roots";
        return 1;
    }

    cout << "Iterations:" << endl;
    
    do{
        counter++;
        // Формула метода хорд: x = (A*F(B) - B*F(A)) / (F(B) - F(A))
        D = (A * F(B) - B * F(A)) / (F(B) - F(A));
        double fD = F(D);
        
        cout << counter << " : " << D << " (f(D) = " << fD << ")" << endl;
        
        if(fabs(fD) < eps){
            break;
        }
        
        if(F(A) * F(D) < 0){
            B = D;
        }
        else{
            A = D;
        }
        
    }while((B - A) >= eps);

    cout << "Result2: " << D << endl;
    cout << "Total iterations: " << counter << endl;
    return 0;
}